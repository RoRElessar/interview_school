document.getElementById('new-student-section').outerHTML = '<%= j render 'create_form' %>';
document.querySelector('#student_section_section_id').addEventListener('change', function () {
  let id = this.value;
  let xhr = new XMLHttpRequest();
  xhr.open('GET', '/sections/' + id, true);
  xhr.setRequestHeader('Accept', 'application/javascript');
  xhr.send();
});
